<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="BigData">
  
  <insert id="updatePentagonBigData" parameterType="hashMap">
  	update pentagon set score_bigdata=#{score} where stock_code=#{code}
  </insert>
  
  </mapper>