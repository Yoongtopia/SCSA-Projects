<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="PentagonAdmin">
	
	<insert id="insertPentagonAdmin" parameterType="PentagonAdmin">
		insert into pentagon
		values (#{stockCode},#{scoreDcf},#{scoreMultiple},#{scoreTrend},#{scoreVar},#{scoreBigdata},#{scoreSum},sysdate(),
	#{pentagonNewsCountEveryDay},
	#{newsScore1},
	#{newsScore2},
	#{newsScore3},
	#{pentagonTodayPrice})
	</insert>
	
</mapper>